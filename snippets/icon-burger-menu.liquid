{% if template.name == "index" %}
  <svg
    viewBox="0 0 24 14"
    fill="none"
    class="fill-white w-6 h-6 hover:fill-gray-400 duration-300"
    xmlns="http://www.w3.org/2000/svg">
    <rect
      width="24"
      height="2"
      rx="1" />
    <rect
      y="6"
      width="24"
      height="2"
      rx="1" />
    <rect
      y="12"
      width="24"
      height="2"
      rx="1" />
  </svg>
{% else %}
  <svg
    viewBox="0 0 24 14"
    fill="none"
    class="fill-black w-6 h-6 hover:fill-gray-400 duration-300"
    xmlns="http://www.w3.org/2000/svg">
    <rect
      width="24"
      height="2"
      rx="1" />
    <rect
      y="6"
      width="24"
      height="2"
      rx="1" />
    <rect
      y="12"
      width="24"
      height="2"
      rx="1" />
  </svg>
{% endif %}